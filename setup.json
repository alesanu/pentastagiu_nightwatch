{
  "src_folders" : ["./tests"],
  "output_folder" : "./reports",
  "custom_commands_path" : "",
  "custom_assertions_path" : "",
  "page_objects_path" : "./pages",
  "globals_path" : "",
  "live_output" : false,
  "parallel_process_delay" : 500,
  "disable_colors" : false,

  "selenium" : {
    "start_process" : false,
    "server_path" : "./bin/",
    "log_path" : "./logs",
    "host" : "127.0.0.1",
    "port" : 4444,
    "cli_args" : {
      "webdriver.chrome.driver" : "./path_to_chromedriver"
    }
  },

  "test_settings" : {
    "theoryLocalChrome" : {
      "globals" : {"env" : "dev"},
      "launch_url" : "http://release.theory.synthesio.net",
      "selenium_host" : "127.0.0.1",
      "selenium_port" : 4444,
      "silent" : true,
      "disable_colors": false,
      "screenshots" : {
        "enabled" : false,
        "path" : ""
      },
      "desiredCapabilities" : {
        "project" : "Synthesio-theoryLocal",
        "browserName" : "chrome",
        "javascriptEnabled" : true,
        "acceptSslCerts" : true
      },
      "skip_testcases_on_fail" : false
    }
  }
